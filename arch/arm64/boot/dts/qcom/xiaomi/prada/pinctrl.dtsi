&tlmm {
	cam_sensor_front1_default {
		config { pins = "gpio40"; };
		mux { pins = "gpio40"; };
	};
	cam_sensor_front1_sleep {
		config { pins = "gpio40"; };
		mux { pins = "gpio40"; };
	};
			cam_sensor_front_comp_pin {
				config {
					bias-pull-up;
					drive-strength = < 0x02 >;
					pins = "gpio41";
				};
				mux {
					function = "gpio";
					pins = "gpio41";
				};
			};
			cam_sensor_front_comp_pin_sleep {
				config {
					bias-pull-up;
					drive-strength = < 0x02 >;
					pins = "gpio41";
				};
				mux {
					function = "gpio";
					pins = "gpio41";
				};
			};
			cam_sensor_rear_comp_pin {
				config {
					bias-pull-up;
					drive-strength = < 0x02 >;
					pins = "gpio61";
				};
				mux {
					function = "gpio";
					pins = "gpio61";
				};
			};
			cam_sensor_rear_comp_pin_sleep {
				config {
					bias-pull-up;
					drive-strength = < 0x02 >;
					pins = "gpio61";
				};
				mux {
					function = "gpio";
					pins = "gpio61";
				};
			};
	cam_sensor_rear_default {
		config { pins = "gpio36"; };
		mux { pins = "gpio36"; };
	};
	cam_sensor_rear_sleep {
		config { pins = "gpio36"; };
		mux { pins = "gpio36"; };
	};
	tlmm_gpio_key {
		gpio_key_active {
			config { pins = "gpio91"; };
			mux { pins = "gpio91"; };
		};
		gpio_key_suspend {
			config { pins = "gpio91"; };
			mux { pins = "gpio91"; };
		};
	};
};

