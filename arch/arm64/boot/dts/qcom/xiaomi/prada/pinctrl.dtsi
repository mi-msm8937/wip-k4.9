&tlmm {
			fpc_irq_active {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					input-enable;
					pins = "gpio46";
				};
				mux {
					function = "gpio";
					pins = "gpio46";
				};
			};
			fpc_reset_active {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					output-high;
					pins = "gpio62";
				};
				mux {
					function = "gpio";
					pins = "gpio62";
				};
			};
			fpc_reset_reset {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					output-low;
					pins = "gpio62";
				};
				mux {
					function = "gpio";
					pins = "gpio62";
				};
			};
			goodix_irq_active {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					input-enable;
					pins = "gpio46";
				};

				mux {
					function = "gpio";
					pins = "gpio46";
				};
			};
			goodix_reset_active {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					output-high;
					pins = "gpio62";
				};
				mux {
					function = "gpio";
					pins = "gpio62";
				};
			};
			goodix_reset_reset {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					output-low;
					pins = "gpio62";
				};
				mux {
					function = "gpio";
					pins = "gpio62";
				};
			};
			goodix_spi_active {
				config {
					bias-disable = < 0x00 >;
					drive-strength = < 0x02 >;
					pins = "gpio0\0gpio1\0gpio2\0gpio3";
				};
				mux {
					function = "blsp_spi1";
					pins = "gpio0\0gpio1\0gpio2\0gpio3";
				};
			};
};
